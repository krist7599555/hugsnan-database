{
  "name": "hugsnan-database",
  "version": "1.0.0",
  "description": "My lovely Nuxt.js project",
  "author": "krist7599555",
  "private": true,
  "scripts": {
    "dev": "kill-port 3000 && cross-env NODE_ENV=development nodemon --watch server --exec ts-node server/index.ts",
    "dev:backend": "PORT=3001 cross-env NODE_ENV=development SKIP_NUXT=1 nodemon --watch server --exec ts-node server/index.ts",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "jest": "SKIP_NUXT=1 JEST=1 jest --ci --verbose --forceExit --detectOpenHandles",
    "clear": "kill -9 $(lsof -i:3000)"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@hiswe/koa-nuxt": "^1.0.0",
    "@line/bot-sdk": "^6.7.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/style-resources": "^0.1.2",
    "@types/axios": "^0.14.0",
    "@types/consola": "^1.0.0",
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-router": "^7.0.40",
    "@types/koa2-cors": "^2.0.1",
    "@types/mongodb": "^3.1.25",
    "@types/supertest": "^2.0.7",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-ignore": "^1.0.1",
    "koa-logger": "^3.2.0",
    "koa-mongo": "^1.8.0",
    "koa-mount": "^4.0.0",
    "koa-multer": "^1.0.2",
    "koa-respond": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.11",
    "markdown-it": "^8.4.2",
    "mavon-editor": "^2.7.3",
    "moment": "^2.24.0",
    "mongodb": "^3.2.3",
    "nuxt": "2.6.2",
    "nuxt-buefy": "^0.3.2",
    "nuxt-fontawesome": "^0.4.0",
    "ts-node": "^8.1.0"
  },
  "devDependencies": {
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3"
  }
