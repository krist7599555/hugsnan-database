  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js --watch server",
    "production": "yarn build:production && yarn upload",
    "build:production": "yarn build:backend && yarn build:frontend && yarn copy:dist && rm -rf ./dist-frontend ./dist-backend",
    "copy:dist": "rm -rf ./dist && mkdir dist && cp -r ./dist-frontend ./dist-backend ./nuxt.config.js ./package.json ./dist",
    "build:frontend": "nuxt build",
    "build:backend": "babel ./server -d ./dist-backend -s",
    "upload": "rsync -r ./dist/* root@128.199.216.159:/root/hugsnan-database",
    "start": "cross-env NODE_ENV=production babel-node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
