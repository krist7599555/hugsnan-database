{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "esmodules": true,
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "transform-es2015-modules-commonjs",
    "@babel/plugin-transform-modules-commonjs"
  ]
}
